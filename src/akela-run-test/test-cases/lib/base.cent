element Test {
    properties {
        solo: Bool
        mute: Bool
    }
    children {
        Field
    }
}

element Struct {
    children {
        Field
    }
}

element Field {
    properties {
        type: Type
        value: Any
    }
}

element Data_type_list {
    children {
        Data_type
    }
}

element Data_type {
    properties {
        type: Type
        align: Natural
    }
}

enum Type {
    Int8
    Int16
    Int32
    Int64
    Nat8
    Nat16
    Nat32
    Nat64
    Real16
    Real32
    Real64
    Bool
}